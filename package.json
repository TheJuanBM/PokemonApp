{
  "name": "pokemonapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier:write": "npx prettier --write \"**/*.+(js|ts|tsx|json)\"",
    "build-android-dev": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "release-apk-dev": "cd android && ./gradlew clean assembleRelease && cd ..",
    "debug-apk-dev": "cd android && ./gradlew clean assembleDebug && cd ..",
    "generate-apk-dev": "npm run build-android-dev && npm run release-apk-dev",
    "bundle-release": "cd android && ./gradlew bundleRelease && cd .."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.11",
    "axios": "^1.2.6",
    "lottie-react-native": "^5.1.4",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-image-colors": "^1.5.1",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-maps": "1.0.0",
    "react-native-modal": "^13.0.1",
    "react-native-progress": "^5.0.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^5.4.1",
    "@testing-library/react-native": "^11.5.1",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest-react": "^0.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "jest": "^26.6.3",
    "jest-mock-axios": "^4.7.0-beta4",
    "metro-react-native-babel-preset": "0.72.3",
    "react-native-dotenv": "^3.4.7",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  }
}
